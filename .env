# Environment Configuration
NODE_ENV=development
VITE_APP_NAME=Mathilde Fleurs
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=https://api.mathilde-fleurs.com
VITE_API_TIMEOUT=10000

# PWA Configuration
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key_here

# Security
VITE_ENCRYPTION_KEY=your_encryption_key_here
VITE_RATE_LIMIT_MAX_ATTEMPTS=30
VITE_RATE_LIMIT_WINDOW_MS=60000

# Analytics
VITE_ANALYTICS_ENABLED=true
VITE_PERFORMANCE_MONITORING=true

# Debug
VITE_DEBUG_MODE=false
VITE_SHOW_PERFORMANCE_METRICS=true
